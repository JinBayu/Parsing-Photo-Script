{
  "manifest_version": 2,

  "name": "parsingPhoto",
  "options_page": "options.html",
  "description": "특정 키워드 기반 웨이보 사진 파싱",
  "version": "1.1.1",
  "background": {
    "page": "background.html"
  },
  
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab","background","downloads","storage"
  ]
}
